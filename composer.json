{
  "name": "phoenixgao/laravel-postgres-extended-schema",
  "description": "Laravel database schema extended, added some PostgreSql features",
  "keywords": [
    "laravel",
    "eloquent",
    "database",
    "mysql",
    "postgresql",
    "schema",
    "blueprint"
  ],
  "authors": [
    {
      "name": "Mirza Pasic",
      "email": "mirza.pasic@edu.fit.ba"
    },
    {
      "name": "Peter Haza",
      "email": "peter.haza@gmail.com"
    },
    {
      "name": "Xiang Gao",
      "email": "phoenix.x.gao@gmail.com"
    }
  ],
  "homepage": "https://github.com/phoenixgao/laravel-postgres-extended-schema",
  "license": "MIT",
  "require": {
    "php": ">=5.5",
    "illuminate/database": "~5.0",
    "ramsey/uuid": "~2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "mockery/mockery": "0.9.*",
    "doctrine/dbal": "~2.5",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Bosnadev\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bosnadev\\Tests\\Database\\": "tests/"
    },
    "classmap": [
      "tests/BaseTestCase.php"
    ]
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.0-dev"
      }
  },
  "minimum-stability": "dev"
}
